Options -Indexes
Options +Multiviews

<IfModule expires_module>
	ExpiresActive On
	ExpiresByType image/gif "access plus 6 month"
	ExpiresByType image/png "access plus 6 month"
	ExpiresByType image/jpg "access plus 6 month"
	ExpiresByType image/jpeg "access plus 6 month"
	ExpiresByType text/css "access plus 6 month"
	ExpiresByType application/javascript "access plus 6 month"
</IfModule>

<IfModule mod_deflate.c>
	AddOutputFilterByType DEFLATE text/plain text/html text/xml text/css application/xml application/javascript application/x-javascript text/javascript text/x-js application/json
</IfModule>